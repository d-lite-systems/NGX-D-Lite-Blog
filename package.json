{
  "name": "ngx-blog-d-lite",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "start": "node dist/srv/index.js",
    "predev": "tsc -p srv",
    "dev": "concurrently \"mongod\" \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p srv\" \"nodemon dist/srv/index.js\"",
    "prod": "concurrently \"mongod\" \"ng build --aot --prod && tsc -p srv && node dist/srv/index.js\"",
    "test": "ng test",
    "postinstall": "tsc -p srv && ng build --aot --prod",
    "testbe": "tsc -p srv && mocha dist/srv/test --exit",
    "lint": "ng lint",
    "linthtml": "htmlhint \"client/**/*.html\"",
    "lintscss": "sass-lint \"client/**/*.scss\" -v",
    "lintbe": "tslint srv/**/**.ts{,x}",
    "e2e": "ng e2e"
  },
